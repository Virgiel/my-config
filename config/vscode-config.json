{
  "workbench.statusBar.visible": true,
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.showTabs": false,
  "workbench.editor.limit.value": 1,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.enablePreview": false,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "workbench.colorCustomizations": {
    "errorLens.warningForeground": "#c18f41d9",
    "errorLens.warningBackground": "#c18f4115",
    "errorLens.infoForeground": "#68948ad9",
    "errorLens.infoBackground": "#0000",
    "errorLens.hintForeground": "#ab6c7dd9",
    "errorLens.hintBackground": "#0000",
    "errorLens.errorForeground": "#b85651d9",
    "errorLens.errorBackground": "#b8565115",
    "editorWarning.background": "#0000",
    "editorInfo.background": "#0000",
    "editorError.background": "#0000"
  },
  "workbench.activityBar.visible": true,
  "window.zoomLevel": 0,
  "whichkey.sortOrder": "alphabetically",
  "whichkey.bindings": [
    {
      "type": "command",
      "name": "commands",
      "key": ";",
      "command": "workbench.action.showCommands"
    },
    {
      "type": "command",
      "name": "comment",
      "key": "/",
      "command": "vscode-neovim.send",
      "args": "<C-/>"
    },
    {
      "type": "command",
      "name": "Focus file explorer",
      "key": "e",
      "command": "workbench.view.explorer"
    },
    {
      "type": "command",
      "name": "Split Horizontal",
      "key": "h",
      "command": "workbench.action.splitEditorDown"
    },
    {
      "type": "command",
      "name": "Focus file finder",
      "key": "f",
      "command": "workbench.action.quickOpen"
    },
    {
      "type": "command",
      "name": "Switch project",
      "key": "p",
      "command": "workbench.action.quickOpenRecent"
    },
    {
      "type": "command",
      "name": "Focus terminal",
      "key": "$",
      "command": "workbench.action.terminal.focus"
    },
    {
      "type": "command",
      "name": "Split Vertical",
      "key": "v",
      "command": "workbench.action.splitEditor"
    },
    {
      "type": "command",
      "name": "Close editor group",
      "key": "x",
      "command": "workbench.action.closeEditorsInGroup"
    },
    {
      "type": "command",
      "name": "Close all editor groups",
      "key": "X",
      "command": "workbench.action.closeAllGroups"
    },
    {
      "type": "command",
      "name": "Open editor history",
      "key": "i",
      "command": "workbench.action.openPreviousEditorFromHistory"
    },
    {
      "type": "command",
      "name": "Toggle zen mode",
      "key": "z",
      "command": "workbench.action.toggleZenMode"
    }
  ],
  "vscode-neovim.neovimExecutablePaths.win32": "C:\\Users\\antoi\\tools\\nvim\\bin\\nvim.exe",
  "vscode-neovim.mouseSelectionStartVisualMode": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontFamily": "Fira Code",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "stylelint.packageManager": "pnpm",
  "python.languageServer": "Pylance",
  "prettier.singleQuote": true,
  "prettier.arrowParens": "avoid",
  "material-icon-theme.saturation": 0.8,
  "material-icon-theme.hidesExplorerArrows": true,
  "markdown.extension.toc.levels": "2..6",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "java.semanticHighlighting.enabled": true,
  "java.refactor.renameFromFileExplorer": "autoApply",
  "java.configuration.checkProjectSettingsExclusions": false,
  "i18n-ally.displayLanguage": "fr",
  "hediet.vscode-drawio.local-storage": "",
  "hediet.vscode-drawio.customFonts": ["Fira Code"],
  "hediet.vscode-drawio.codeLinkActivated": false,
  "gruvboxMaterial.lightContrast": "hard",
  "gruvboxMaterial.italicKeywords": true,
  "gruvboxMaterial.darkWorkbench": "flat",
  "gruvboxMaterial.darkCursor": "orange",
  "gruvboxMaterial.darkContrast": "hard",
  "gruvboxMaterial.colorfulSyntax": true,
  "grammarly.autoActivate": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "files.autoSave": "afterDelay",
  "extensions.autoUpdate": true,
  "explorer.openEditors.visible": 0,
  "explorer.incrementalNaming": "smart",
  "errorLens.margin": "1.5ch",
  "errorLens.fontWeight": "400",
  "errorLens.fontSize": "0.8em",
  "errorLens.exclude": ["Unknown word"],
  "editor.suggestSelection": "first",
  "editor.minimap.enabled": false,
  "editor.linkedEditing": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.fontFamily": "Fira Code",
  "editor.accessibilitySupport": "off",
  "diffEditor.ignoreTrimWhitespace": false,
  "clangd.path": "c:\\Users\\antoi\\AppData\\Roaming\\Code\\User\\globalStorage\\llvm-vs-code-extensions.vscode-clangd\\install\\10.0.0\\clangd_10.0.0\\bin\\clangd.exe",
  "clangd.onConfigChanged": "restart",
  "cSpell.language": "fr,fr-FR,en-US",
  "cSpell.enableFiletypes": ["pseudocode", "svelte", "toml", "vue"],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": []
}
