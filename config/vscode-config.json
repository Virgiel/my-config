  "hediet.vscode-drawio.codeLinkActivated": false,
  "gruvboxMaterial.lightContrast": "hard",
  "gruvboxMaterial.italicKeywords": true,
  "gruvboxMaterial.darkWorkbench": "flat",
  "gruvboxMaterial.darkCursor": "orange",
  "gruvboxMaterial.darkContrast": "hard",
  "grammarly.autoActivate": false,
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "files.autoSave": "afterDelay",
  "extensions.autoUpdate": true,
  "explorer.openEditors.visible": 0,
  "explorer.incrementalNaming": "smart",
  "errorLens.margin": "1.5ch",
  "errorLens.fontWeight": "400",
  "errorLens.fontSize": "0.8em",
  "errorLens.exclude": ["Unknown word"],
  "editor.suggestSelection": "first",
  "editor.minimap.enabled": false,
  "editor.linkedEditing": true,
  "editor.formatOnType": true,
  "editor.formatOnSave": true,
  "editor.fontSize": 13,
  "editor.fontLigatures": true,
  "editor.fontFamily": "Fira Code",
  "editor.accessibilitySupport": "off",
  "clangd.path": "c:\\Users\\antoi\\AppData\\Roaming\\Code\\User\\globalStorage\\llvm-vs-code-extensions.vscode-clangd\\install\\10.0.0\\clangd_10.0.0\\bin\\clangd.exe",
  "clangd.onConfigChanged": "restart",
  "cSpell.language": "fr,fr-FR,en-US,en-GB",
  "cSpell.enableFiletypes": [
    "git-rebase",
    "makefile",
    "pseudocode",
    "shellscript",
    "svelte",
    "toml",
    "vue"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[markdown]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80,
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[latex]": {
    "editor.wordWrap": "wordWrapColumn",
    "editor.wordWrapColumn": 80
  },
  "workbench.productIconTheme": "fluent-icons",
  "prettier.proseWrap": "always",
  "terminal.integrated.profiles.windows": {
    "nu": {
      "path": "nu.exe",
      "args": [""]
    }
  },
  "terminal.integrated.defaultProfile.windows": "nu",
  "cSpell.userWords": ["depolymerizer", "eytzingerize"],
  "todo-tree.general.tags": [
    "BUG",
    "HACK",
    "FIXME",
    "TODO",
    "XXX",
    "[ ]",
    "[x]"
  ],
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
