{
  "workbench.statusBar.visible": true,
  "workbench.list.automaticKeyboardNavigation": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.showTabs": false,
  "workbench.editor.limit.value": 1,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.enablePreview": false,
  "workbench.colorTheme": "Gruvbox Material Dark",
  "workbench.colorCustomizations": {
    "errorLens.warningForeground": "#c18f41d9",
    "errorLens.warningBackground": "#c18f4115",
    "errorLens.infoForeground": "#68948ad9",
    "errorLens.infoBackground": "#0000",
    "errorLens.hintForeground": "#ab6c7dd9",
    "errorLens.hintBackground": "#0000",
    "errorLens.errorForeground": "#b85651d9",
    "errorLens.errorBackground": "#b8565115",
    "editorWarning.background": "#0000",
    "editorInfo.background": "#0000",
    "editorError.background": "#0000"
  },
  "whichkey.sortOrder": "alphabetically",
  "whichkey.bindings": [
    {
      "type": "command",
      "name": "commands",
      "key": ";",
      "command": "workbench.action.showCommands"
    },
    {
      "type": "command",
      "name": "File finder",
      "key": "f",
      "command": "workbench.action.quickOpenPreviousEditor"
    },
    {
      "type": "command",
      "name": "File explorer",
      "key": "F",
      "command": "workbench.view.explorer"
    },
    {
      "type": "command",
      "name": "Switch project",
      "key": "p",
      "command": "workbench.action.quickOpenRecent"
    },
    {
      "type": "command",
      "name": "Open terminal",
      "key": "$",
      "command": "terminal.focus"
    },
    {
      "type": "command",
      "name": "Create terminal",
      "key": "Â£",
      "command": "workbench.action.terminal.new"
    },
    {
      "type": "command",
      "name": "Open breadcrumbs",
      "key": "b",
      "command": "breadcrumbs.focusAndSelect"
    },
    {
      "type": "command",
      "name": "Go to symbol in editor",
      "key": "s",
      "command": "workbench.action.gotoSymbol"
    },
    {
      "type": "command",
      "name": "Go to symbol in workspace",
      "key": "S",
      "command": "workbench.action.showAllSymbols"
    },
    {
      "type": "command",
      "name": "Next error",
      "key": "e",
      "command": "editor.action.marker.nextInFiles"
    },
    {
      "type": "command",
      "name": "Next error",
      "key": "E",
      "command": "editor.action.marker.prevInFiles"
    },
    {
      "type": "command",
      "name": "Split Vertical",
      "key": "v",
      "command": "workbench.action.splitEditor"
    },
    {
      "type": "command",
      "name": "Split Horizontal",
      "key": "h",
      "command": "workbench.action.splitEditorDown"
    },
    {
      "type": "command",
      "name": "Close editor group",
      "key": "d",
      "command": "workbench.action.closeEditorsInGroup"
    },
    {
      "type": "command",
      "name": "Close all editor groups",
      "key": "D",
      "command": "workbench.action.closeAllGroups"
    },
    {
      "type": "command",
      "name": "Toggle zen mode",
      "key": "z",
      "command": "workbench.action.toggleZenMode"
    }
  ],
  "vscode-neovim.neovimExecutablePaths.win32": "C:\\Users\\antoi\\scoop\\apps\\neovim\\current\\bin\\nvim.exe",
  "vscode-neovim.mouseSelectionStartVisualMode": true,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontFamily": "Fira Code",
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "stylelint.packageManager": "pnpm",
  "python.languageServer": "Pylance",
  "prettier.singleQuote": true,
  "prettier.arrowParens": "avoid",
  "material-icon-theme.saturation": 0.8,
  "material-icon-theme.hidesExplorerArrows": true,
  "markdown.extension.toc.levels": "2..6",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "java.semanticHighlighting.enabled": true,
  "java.refactor.renameFromFileExplorer": "autoApply",
  "java.configuration.checkProjectSettingsExclusions": false,
  "i18n-ally.displayLanguage": "fr",
  "hediet.vscode-drawio.local-storage": "",
